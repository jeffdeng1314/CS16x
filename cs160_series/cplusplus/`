#include <iostream>
#include <cstring>

using std::cout;
using std::cin;
using std::endl;

int main(){

	int rows = 10;
	int cols = 5;

	int **names = new int*[cols];
	for(int i =0; i < cols; i++){
		names[i] = new int[rows];
		cout <<"Cols: "<< i;
		for(int j = 0; j < rows; j ++){

			names[i][j] = j;
cout <<endl;
			cout <<"rows: "<< j;
		}
		cout <<endl;
	cout <<'\n';
		}
///*
	        for(int i =0; i < cols; i++){

                      delete [] names[i];
}			
			delete [] names;
			names = NULL;

//*/

	return 0;
}
